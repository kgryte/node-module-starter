    "helper",
    "generator",
    "skeleton",
    "tools"
  ],
  "bugs": {
    "url": "https://github.com/kgryte/node-module-starter-kit/issues"
  },
  "dependencies": {
    "@kgryte/editorconfig": "^1.0.3",
